{
  "name": "reactjs-wp",
  "version": "2.2.0",
  "description": "A starting point for building web applications with ReactJS using ES6, Webpack and PostCSS",
  "scripts": {
    "build": "webpack",
    "build:prod": "export NODE_ENV='production' && webpack",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint 'App/**/*.css'",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "start": "npm run watch",
    "watch": "webpack-dev-server"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/reactjs-wp.git"
  },
  "keywords": [
    "reactjs",
    "es6",
    "es2015",
    "webpack",
    "postcss"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/reactjs-wp/issues"
  },
  "homepage": "https://github.com/nimedev/reactjs-wp#readme",
  "engines": {
    "npm": ">=3.10.9",
    "node": ">=6.9.2 <7.0.0"
  },
  "dependencies": {
    "niduscss-base": "1.0.1",
    "niduscss-elements-form": "1.3.0",
    "niduscss-elements-link": "1.1.1",
    "niduscss-elements-typography": "1.1.2",
    "niduscss-lib-mixins": "1.4.0",
    "niduscss-utils-align": "1.3.1",
    "niduscss-utils-buttons": "1.0.1",
    "niduscss-utils-color": "1.0.1",
    "niduscss-utils-flex": "1.1.0",
    "niduscss-utils-shadow": "1.3.2",
    "niduscss-utils-state": "1.1.1",
    "niduscss-utils-text": "1.3.2",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "devDependencies": {
    "autoprefixer": "6.6.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "clean-webpack-plugin": "0.1.14",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
